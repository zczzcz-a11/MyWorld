{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue","src/assets/js/region.json","src/**/**/.vue"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
    },
    "module": "ESNext",
    "lib": ["es2018"],
    "ignoreDeprecations": "5.0",
  }
}
